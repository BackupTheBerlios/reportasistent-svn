<!-- formulation patterns for Above Average -->

<fp id="fp101" quantifier="AAI" numofant="1+" numofsuc="1+" numofcon="0">
  01. Frekvence <literal role="S" typeofphrase="N" case="2"/>
  je o <param name="P"/> % vyšší mezi <fpa symbol="Y" case="7" number="pl"/>
  než v celé kolekci.
</fp>

<fp id="fp102" quantifier="AAI" numofant="1+" numofsuc="1+" numofcon="1+">
  02. Frekvence <literal role="S" typeofphrase="N" case="2"/>
  je o <param name="P"/> % vyšší mezi <fpa symbol="Y" case="7" number="pl"/>
  než v celé kolekci. Platí pro <param name="B"/> <ename symbol="E" case="2" number="pl"/>
  <ename symbol="Erel" number="pl"/> splňuji tyto podmínky:
    <ename symbol="E" case="1" number="sg"/>
	<literal role="C" typeofphrase="V"/>.
</fp>

<fp id="fp103" quantifier="AAI" numofant="1+" numofsuc="1+" numofcon="0">
  03. Frekvence
 <ename symbol="E" number="pl" case="2"/>,
 <ename symbol="Erel" number="pl"/>
 <literal role="S" typeofphrase="V" number="pl"/>
  je o <param name="P"/> % vyšší mezi <fpa symbol="Y" case="7" number="pl"/>
  než v celé kolekci.
</fp>

<fp id="fp104" quantifier="AAI" numofant="1+" numofsuc="1+" numofcon="1+">
