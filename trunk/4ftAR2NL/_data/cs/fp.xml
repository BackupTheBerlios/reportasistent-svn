<!-- formulation patterns for Founded Implication -->

<fp id="fp1" quantifier="FUI" numofant="1+" numofsuc="1+" numofcon="0">
  01. <fpa symbol="X"/> <ename symbol="Eptc0" case="2" number="pl"/>
  <ename symbol="E" case="2" number="pl"/>
  potvrzuje tuto závislost: jestliže <floatslot id="1" role="A" occno="first"/>
  <ename symbol="E"/>
  <literal role="A" typeofphrase="V"/>, pak
  <floatslot id="1" role="S" occno="first"/>
  <optionaltext>
    <choosetext>
      <variant>také</variant>
      <variant>rovněž</variant>
    </choosetext>
  </optionaltext>
  <literal role="S" typeofphrase="V"/>.
</fp>

