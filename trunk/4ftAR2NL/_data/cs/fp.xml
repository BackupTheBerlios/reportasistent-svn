	<ename symbol="E" case="2" number="pl"/>,
	<ename symbol="Erel" number="pl"/>
	<floatslot id="1" role="A" occno="first"/>
	<literal role="A" typeofphrase="V" number="pl"/>
	nebo
	<literal role="S" typeofphrase="V" number="pl"/>,
	<param name="P"/>% (konkrétně <param name="B"/>)
